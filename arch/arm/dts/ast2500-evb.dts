/dts-v1/;

#include "ast2500-u-boot.dtsi"

/ {
	memory {
		device_type = "memory";
		reg = <0x80000000 0x20000000>;
	};

	chosen {
		stdout-path = &uart5;
		fru-eeprom = &eeprom0;
	};

	aliases {
		i2c1 = &i2c0;
		i2c4 = &i2c3;
		i2c8 = &i2c7;
	};
};

&uart5 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&sdrammc {
	clock-frequency = <400000000>;
};

&wdt1 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&wdt2 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&wdt3 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c3 {
	status = "okay";

	eeprom0: eeprom@50 {
		compatible = "atmel,24c08a", "i2c-eeprom";
		reg = <0x50>;
	};
};

&i2c7 {
	status = "okay";
};
