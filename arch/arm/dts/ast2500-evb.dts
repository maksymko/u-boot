/dts-v1/;

#include "ast2500-u-boot.dtsi"

/ {
	memory {
		device_type = "memory";
		reg = <0x80000000 0x20000000>;
	};

	chosen {
		stdout-path = &uart5;
	};
};

&uart5 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&sdrammc {
	clock-frequency = <400000000>;
};

&wdt1 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&wdt2 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&wdt3 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c3 {
	status = "okay";

	eeprom0: eeprom@50 {
		compatible = "atmel,24c08a", "i2c-eeprom";
		reg = <0x50>;
	};
};

&i2c7 {
	status = "okay";
};

&mac0 {
	compatible = "aspeed,ast2500-nic";
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_mac1link_default>, <&pinctrl_mdio1_default>;
};

&mac1 {
	compatible = "aspeed,ast2500-nic";
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_mac2link_default>, <&pinctrl_mdio2_default>;
};
